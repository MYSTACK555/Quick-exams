---
title: Quick exams | Shop
layout: base.njk
pagination:
    data: languages
    size: 1
    alias: languages
permalink: /{{ languages.language }}/shop.html
---

<h1>{{languages.traductionFile.h1_shop}}</h1>
<div class="allItem">
    {% for item in marketAPI %}
    <section class="shopItem" name="{{item.code}}">
        <div name="title">{{item.name}}</div>
        <div name="description">{{item.description}}</div>
        <div name="priceButton">
            <div name="priceTag">
                <div name="price">{{item.prix}}{{item.devise}}</div>
                <div name="periode">{{item.frequence}}</div>
            </div>
            <button type="button" >{{languages.traductionFile.shop_addToCart}}</button>
        </div>
    </section>
    {%endfor%}
</div>